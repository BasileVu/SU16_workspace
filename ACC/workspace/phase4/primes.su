int isPrime(int n) {

    if (n < 2) {
        return 0;
    }
    
    if (n == 2) {
        return 1;
    }
    
    int j;
    j = 3;
    
    while (j < (n/2) + 1) { 
        if (n % j == 0) {
            return 0;
        }
    
        j = j + 1;
    }
    return 1;
}

